{
  "dashboard": {
    "id": null,
    "title": "KokoroTTS API - Rate Limiting Dashboard",
    "tags": ["kokorotts", "rate-limiting", "api"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Request Rate & Blocks",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(kokorotts_requests_total[1m]) * 60",
            "legendFormat": "Requests/min"
          },
          {
            "expr": "rate(kokorotts_requests_blocked_total[1m]) * 60",
            "legendFormat": "Blocked/min"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "min": 0
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Block Rate (%)",
        "type": "gauge",
        "targets": [
          {
            "expr": "(rate(kokorotts_requests_blocked_total[5m]) / rate(kokorotts_requests_total[5m])) * 100",
            "legendFormat": "Block Rate"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 10},
                {"color": "red", "value": 25}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Character Usage",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(kokorotts_characters_processed_total[1m]) * 60",
            "legendFormat": "Characters/min"
          },
          {
            "expr": "kokorotts_character_limit_per_hour",
            "legendFormat": "Hourly Limit"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Concurrent Connections",
        "type": "timeseries",
        "targets": [
          {
            "expr": "kokorotts_concurrent_requests",
            "legendFormat": "Current Connections"
          },
          {
            "expr": "kokorotts_max_concurrent_limit",
            "legendFormat": "Max Allowed"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 5,
        "title": "Top Rate Limited IPs",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, sum by (ip) (rate(kokorotts_requests_blocked_total[1h])))",
            "format": "table",
            "instant": true
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 6,
        "title": "Rate Limit Types",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (limit_type) (rate(kokorotts_requests_blocked_total[1h]))",
            "legendFormat": "{{limit_type}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 7,
        "title": "Redis Connection Status",
        "type": "stat",
        "targets": [
          {
            "expr": "kokorotts_redis_connected",
            "legendFormat": "Redis Status"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"options": {"0": {"text": "Disconnected", "color": "red"}}},
              {"options": {"1": {"text": "Connected", "color": "green"}}}
            ]
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s"
  }
}